{
    "background": {
        "scripts": [
          "background.js",
          "password.js"
        ]
    },
    "content_scripts": [
      {
        "matches": [
          "*://facebook.com/*",
          "*://*.facebook.com/*",
          "*://*.reddit.com/*",
          "*://mgsloan.com/start-page/index.html*"
        ],
        "js": ["content.js"],
        "all_frames": true
      }
    ],
    "icons": {
        "128": "icon-128.png",
        "16": "icon-16.png"
    },
    "manifest_version": 2,
    "minimum_chrome_version": "23",
    "name": "SiteBlocker",
    "permissions": [
        "webRequest",
        "webRequestBlocking",
        "<all_urls>"
    ],
    "version": "0.0.1"
}
